{
  "name": "@govuk-one-login/frontend-ui",
  "version": "4.1.2",
  "description": "",
  "main": "build/cjs/backend/index.cjs",
  "module": "build/esm/backend/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "test": "jest --coverage src",
    "test:visual": "npx playwright install --with-deps && playwright test browser-tests/functional-tests",
    "test:visual:update": "npx playwright install --with-deps && playwright test browser-tests/functional-tests --update-snapshots"
  },
  "files": [
    "build/",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/govuk-one-login/govuk-one-login-frontend.git",
    "directory": "packages/frontend-ui"
  },
  "keywords": [
    "govuk-one-login",
    "gds",
    "govuk-onelogin",
    "govuk one login"
  ],
  "author": "DI Frontend Capability Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/govuk-one-login/govuk-one-login-frontend/issues"
  },
  "homepage": "https://github.com/govuk-one-login/govuk-one-login-frontend#readme",
  "dependencies": {
    "js-yaml": "^4.1.1",
    "pino": "^10.1.0"
  },
  "peerDependencies": {
    "@govuk-one-login/frontend-analytics": " ^3.0.1 || ^4.0.3",
    "express": "^5.1.0 || >= 4.21.2",
    "govuk-frontend": "^4.10.1 || ^5.0.0"
  },
  "exports": {
    ".": {
      "import": "./build/esm/backend/index.js",
      "require": "./build/cjs/backend/index.cjs"
    },
    "./frontend": {
      "import": "./build/esm/frontend/index.js",
      "require": "./build/cjs/frontend/index.cjs"
    }
  },
  "types": "./build/esm/backend/index.d.ts",
  "optionalDependencies": {
    "@govuk-one-login/frontend-analytics": "^4.0.5",
    "hmpo-components": "^7.1.0"
  }
}
