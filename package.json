{
  "name": "govuk-one-login-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "test": "jest --config=jest.config.js --passWithNoTests",
    "lint": "nx affected --target=lint",
    "lint:fix": "nx affected --target=lint --fix",
    "format": "nx affected --target=format",
    "format:check": "nx affected --target=format:check",
    "prep:commit": "npm run format && npm run lint && git add .",
    "prepare": "husky"
  },
  "dependencies": {
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@nrwl/cli": "^15.9.3",
    "@nrwl/eslint-plugin-nx": "^19.0.4",
    "@nrwl/linter": "^19.0.4",
    "@nrwl/node": "^19.0.4",
    "@nrwl/workspace": "^19.0.4",
    "@nx/js": "18.3.2",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "nx": "18.3.2",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-editorconfig": "^4.0.3",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11"
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
