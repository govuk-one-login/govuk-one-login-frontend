{
  "name": "govuk-one-login-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nx run-many --target=build",
    "test": "nx affected --target=test",
    "lint": "nx affected --target=lint",
    "lint:fix": "nx affected --target=lint --fix",
    "format": "nx affected --target=format",
    "format:check": "nx affected --target=format:check",
    "prep:commit": "npm run format && npm run lint",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@nrwl/cli": "^15.9.3",
    "@nx/eslint": "20.6.2",
    "@nx/eslint-plugin": "20.6.2",
    "@nx/jest": "20.6.2",
    "@nx/js": "20.6.2",
    "@nx/node": "20.6.2",
    "@nx/workspace": "20.6.2",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "canvas": "^3.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-editorconfig": "^4.0.3",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.2.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "nx": "20.6.2",
    "prettier": "3.5.3"
  },
  "overrides": {
    "jest-environment-jsdom": {
      "canvas": "$canvas"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
