{
  "name": "@govuk-one-login/frontend-asset-loader",
  "version": "0.0.5",
  "description": "Loads cached assets",
  "main": "build/cjs/index.cjs",
  "module": "build/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": "./build/esm/index.js",
    "require": "./build/cjs/index.cjs"
  },
  "type": "module",
  "scripts": {
    "test": "jest -c --coverage",
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/govuk-one-login/govuk-one-login-frontend.git"
  },
  "keywords": [
    "govuk-one-login",
    "gds",
    "govuk-onelogin",
    "govuk one login"
  ],
  "author": "DI Frontend Capability Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/govuk-one-login/govuk-one-login-frontend/issues"
  },
  "homepage": "https://github.com/govuk-one-login/govuk-one-login-frontend#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "fast-glob": "^3.3.3",
    "jest": "^29.5.0",
    "rollup": "4.37.0",
    "rollup-plugin-typescript2": "0.36.0",
    "ts-jest": "29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2"
  }
}
